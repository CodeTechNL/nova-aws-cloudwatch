{
    "name": "codetechnl/nova-aws-cloudwatch",
    "description": "Laravel Nova 4 | Get the logs from AWS CloudWatch",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Donny van Grondelle",
            "email": "info@codetech.nl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Codetechnl\\NovaAwsCloudwatch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codetechnl\\NovaAwsCloudwatch\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
